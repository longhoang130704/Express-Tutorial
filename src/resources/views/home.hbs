<div class="mt-4">
  <div class="row">
      {{#each data}}
    <div class="col-sm-6 col-lg-4">

      <div class="card">
        <a href="/course/{{this.slug}}">
          <img src="{{this.img}}" class="card-img-top" alt="...">
        </a>
        <div class="card-body">
          <a href="/course/{{this.slug}}">
            <h5 class="card-title">{{this.title}}</h5>
          </a>
          <p class="card-text">{{this.decription}}</p>
          <a href="#" class="btn btn-primary">Add to Cart</a>
        </div>
      </div>

    </div>
      {{/each}}
  </div>
</div>